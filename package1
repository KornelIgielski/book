# book
package com.company;

public class Book {
    private String author, title;
    public boolean isBorrowed;

    public Book(String author, String title) {
        this.author = author;
        this.title = title;
        isBorrowed=false;
    }
    public String toString(){
        return author + " " + "\"" + title + "\"";
    }
}
